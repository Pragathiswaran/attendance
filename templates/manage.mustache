<!-- local/attendance/templates/activity_summary.mustache -->

<!-- Single button -->


<table class="generaltable">
    <thead>
        <tr>
            <th>{{#str}}userid, local_attendance{{/str}}</th>
            <th>{{#str}}username, local_attendance{{/str}}</th>
            <th>{{#str}}courseid, local_attendance{{/str}}</th>
            <th>{{#str}}coursename, local_attendance{{/str}}</th>
            <th>{{#str}}sessiondate, local_attendance{{/str}}</th>
            <th>{{#str}}sessionstart, local_attendance{{/str}}</th>
            <th>{{#str}}sessionend, local_attendance{{/str}}</th>
            <th>{{#str}}sessionduration, local_attendance{{/str}}</th>
        </tr>
    </thead>
    <tbody>
        {{#dateWiseAccess}}
        <tr>
            <td>{{userid}}</td>
            <td>{{username}}</td>
            <td>{{courseid}}</td>
            <td>{{coursename}}</td>
            <td>{{date}}</td>
            <td>{{start_time}}</td> <!-- Adjusted for consistency -->
            <td>{{end_time}}</td>
            <td>{{duration}}</td>
        </tr>
        {{/dateWiseAccess}}
    </tbody>
</table>

<button class="btn btn-success">submit</button>

