<style>
  #title {
    font-size: 40px;
    font-family: timesnewroman;
    display: inline-block;
  }

  #title:hover {
    color: orangered;
  }

  .btn-group {
    margin-left: 10px;
    display: inline-block;
  }

  .btn-group:first-child {
    margin-left: 0;
  }

  .btn-group:last-child {
    margin-right: 0;
  }

  .btn-search.fa.fa-search {
    width: 40px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    gap: 10px;
  }

  .search {
    height: 35px;
    max-width: 300px;
    display: inline-flex;
    margin-left: 10px;
  }

  .search-text {
    padding: 10px;
    width: 220px;
    outline: none;
    border: 1px solid rgb(77, 77, 255);
    border-radius: 5px 0 0 5px;
    transition: 0.5s;
    background-color: rgb(77, 77, 255, 0.1);
  }

  .search-text::placeholder {
    color: rgb(77, 77, 255);
  }

  .search-text:focus {
    background: #fff;
  }

  .btn-search {
    background-color: rgb(77, 77, 255);
    color: white;
    border-radius: 0 5px 5px 0;
    border: 1px solid rgb(77, 77, 255);
  }

  thead {
    text-align: center;
    background-color: #2e2e2e;
    color: #fff;
    font-size: 16px;
  }

  tbody {
    font-size: 14px;
    text-align: center;
  }

  .incompleted {
    background-color: #f6c4bf;
  }

  .search-text.no-match::placeholder {
    color: red;
  }

  .filter-container {
    display: flex;
    justify-content: flex-end;
    margin: 20px;
  }

  .btn-group ml-1
  {
    margin-top:2px;
  }
</style>

<div class="container">
 <h1 id='title'></h1>
  <div class="d-flex flex-row">
    <div class="btn-group ml-1 ">
      <button class="btn btn-warning dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <span class="glyphicon glyphicon-book"></span> Course <span class="caret"></span>
      </button>
      <ul class="dropdown-menu">
          {{#coursetitleData}}
          <li class="btn btn-value">{{coursenames}}</li>
          {{/coursetitleData}}
      </ul>
    </div>

    <div class="btn-group ml-1">
      <button class="btn btn-warning dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <span class="glyphicon glyphicon-print"></span> Report <span class="caret"></span>
      </button>
      <ul class="dropdown-menu">
          <li class="btn btn-options">Login</li>
          <li class="btn btn-options">Course</li>
          <li class="btn btn-options">Quiz</li>
          <li class="btn btn-options">Assignment</li>
      </ul>
    </div>

    <div class="btn-group ml-1 ">
      <button class="btn btn-warning dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" onclick=openPopup()>
          <span class="glyphicon glyphicon-stats"></span> Chart <span class="caret"></span>
      </button>
      <ul class="dropdown-menu">
          <li class="btn btn-chart" onclick="location.href='{{chart}}'">Login</li>
          <li class="btn btn-chart">Course Access</li>
          <li class="btn btn-chart">Quiz Access</li>
          <li class="btn btn-chart">Assignment Access</li>
      </ul>
    </div>


    <div class="btn-group ml-1">
      <button class="btn btn-warning dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
          <i class="icon-download"></i> Export <span class="caret"></span>
      </button>
      <ul class="dropdown-menu">
          <li><button class="btn btn-filepdf">PDF</button></li>
          <li><button class="btn btn-file" onclick="exportToExcel()">EXCEL</button></li>
          <li><button class="btn btn-file" onclick="copyTable()">COPY</button></li>
          <li><button class="btn btn-file" onclick="exportTocsv()">CSV</button></li>
          <li><button class="btn btn-file" onclick="printTable()">PRINT</button></li>
          <li><button class="btn btn-file" onclick="location.href='{{emailurl}}'">EMAIL</button></li>
      </ul>
    </div>
  </div>

  <div class="filter-container">
    <div class="search">
      <input type="text" class="search-text" placeholder="Type to Filter . . .">
      <button class="btn btn-search fa fa-search" type="button"></button>
    </div>
  </div>
</div>
<table id="example" class="table table-bordered table-condensed">
</table>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.4/jspdf.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf-autotable/3.5.13/jspdf.plugin.autotable.min.js"></script>